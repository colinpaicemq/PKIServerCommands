   "criteria. "
  )
  TAB( 8 , 8 , 64 , "Notify email cannot be sent. ")
}
else
  printf("Unknown function %s\n",function);

if (strlen(pReturn) == 0)
{
 printf("Unknown rc. Function %s SAFRC %d RACFRC %d RACFRS %d\n",
        function,safrc,racfrc,racfrs);
 pReturn = "Unknown values \n";
 }
return pReturn;
}
