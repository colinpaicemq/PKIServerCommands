else
if (strcmp(function,"QRECOVER"   )== 0)
{
  if (1 == 0)
  { // do nothing)
  }
  TAB( 8 , 8 , 40 , "Results list area missing. ")
  TAB( 8 , 8 , 44 , "Results list area too small. ")
  TAB( 8 , 8 , 48 ,"Incorrect requesterâ€™s email address "
